{
  "build": {
    "dockerfile": "./Dockerfile",
    "context": "."
  },
  "remoteUser": "coder",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:1": {
      "uid": "1000",
      "gid": "1000",
      "username": "coder",
      "installZsh": "true",
      "upgradePackages": "true"
    },
    "ghcr.io/devcontainers/features/git:1": {
      "ppa": "true"
    },
    "ghcr.io/devcontainers/features/git-lfs:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/trxcllnt/devcontainer-features/gitlab-cli:2": {},
    "ghcr.io/trxcllnt/devcontainer-features/cmake:2": {},
    "ghcr.io/trxcllnt/devcontainer-features/ninja:2": {},
    "ghcr.io/trxcllnt/devcontainer-features/sccache:2": {
      "version": "0.4.0-pre.6"
    },
    "ghcr.io/trxcllnt/devcontainer-features/llvm:2": {},
    "ghcr.io/trxcllnt/devcontainer-features/rust:2": {},
    "ghcr.io/trxcllnt/devcontainer-features/mambaforge:2": {},
    "ghcr.io/trxcllnt/devcontainer-features/utils:2": {}
  },
  "overrideFeatureInstallOrder": [
    "ghcr.io/devcontainers/features/common-utils",
    "ghcr.io/devcontainers/features/git",
    "ghcr.io/devcontainers/features/git-lfs",
    "ghcr.io/devcontainers/features/github-cli",
    "ghcr.io/trxcllnt/devcontainer-features/gitlab-cli",
    "ghcr.io/trxcllnt/devcontainer-features/cmake",
    "ghcr.io/trxcllnt/devcontainer-features/ninja",
    "ghcr.io/trxcllnt/devcontainer-features/sccache",
    "ghcr.io/trxcllnt/devcontainer-features/llvm",
    "ghcr.io/trxcllnt/devcontainer-features/rust",
    "ghcr.io/trxcllnt/devcontainer-features/mambaforge",
    "ghcr.io/trxcllnt/devcontainer-features/utils"
  ]
}
